{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "repo_full_name": {
        "type": "string",
        "pattern": "^[A-Za-z0-9_.-]+/[A-Za-z0-9_.-]+$",
        "maxLength": 300
      }
    },
    "required": ["repo_full_name"],
    "additionalProperties": true
  }
}
